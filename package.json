{
  "name": "extract-react-intl-messages",
  "version": "1.0.2",
  "description": "Extract react-intl messages",
  "license": "MIT",
  "repository": "akameco/extract-react-intl-messages",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "https://akameco.github.io"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "fmt": "prettier --write '**/*.{json,js,md}'",
    "example": "./cli.js -l=en,ja -o example/i18n -d en 'example/**/*.js'",
    "lint": "eslint index.js cli.js ./extract-react-intl/index.js",
    "test": "npm run lint && jest"
  },
  "bin": {
    "extract-react-intl-messages": "cli.js",
    "extract-messages": "cli.js"
  },
  "files": [
    "extract-react-intl/index.js",
    "index.js",
    "cli.js"
  ],
  "keywords": [
    "react",
    "i18n",
    "intl",
    "react-intl",
    "extract",
    "json",
    "messages"
  ],
  "dependencies": {
    "@babel/core": "^7.5.5",
    "babel-plugin-react-intl": "3.5.1",
    "extract-react-intl": "^0.9.0",
    "flat": "^4.1.0",
    "glob": "^7.1.4",
    "js-yaml": "^3.13.1",
    "load-json-file": "^6.2.0",
    "lodash.merge": "^4.6.2",
    "lodash.mergewith": "^4.6.2",
    "lodash.pick": "^4.4.0",
    "meow": "^5.0.0",
    "mkdirp": "^0.5.1",
    "pify": "^4.0.1",
    "read-babelrc-up": "^0.4.0",
    "sort-keys": "^3.0.0",
    "write-json-file": "^4.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "all-contributors-cli": "^6.8.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-react-intl-auto": "^2.1.0",
    "eslint": "^6.1.0",
    "eslint-config-precure": "^4.18.1",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-intl": "^3.1.10",
    "temp-write": "^4.0.0",
    "tempy": "^0.3.0"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
